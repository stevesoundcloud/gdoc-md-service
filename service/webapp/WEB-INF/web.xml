<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC "-//Oracle Corporation//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
  <servlet>
    <servlet-name>my-app</servlet-name>
    <servlet-class>com.google.bazel.example.app.MyAppServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>my-app</servlet-name>
    <url-pattern>/testapp/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>my-cron</servlet-name>
    <servlet-class>com.google.bazel.example.app.MyCronServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>my-cron</servlet-name>
    <url-pattern>/testcron/*</url-pattern>
  </servlet-mapping>
</web-app>
